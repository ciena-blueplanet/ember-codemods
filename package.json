{
  "name": "pretty-good-ember-codemods",
  "version": "0.1.0",
  "description": "A collection of pretty good codemods for Ember",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "prepare": "npm run build",
    "test": "npm run lint && glob -c \"node --import tsx --test\" \"src/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciena-blueplanet/pretty-good-ember-codemods.git"
  },
  "keywords": [
    "codemod",
    "ember"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/pretty-good-ember-codemods/issues"
  },
  "homepage": "https://github.com/ciena-blueplanet/pretty-good-ember-codemods#readme",
  "engines": {
    "node": ">= 18.20"
  },
  "bin": {
    "pretty-good-ember-codemods": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "imports": {
    "#*": {
      "pretty-good-ember-codemods-dev": "./src/*",
      "default": "./dist/*"
    }
  },
  "dependencies": {
    "jscodeshift": "^17.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@eslint/js": "^9.19.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.10.10",
    "ast-types": "^0.14.2",
    "eslint": "^9.19.0",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
